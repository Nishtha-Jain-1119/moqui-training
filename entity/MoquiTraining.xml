<?xml version="1.0" encoding="UTF-8"?>

<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">
    <entity entity-name="MoquiTraining" package="moqui.training">
        <field name="trainingId" type="id" is-pk="true"></field>
        <field name="trainingName" type="text-medium"></field>
        <field name="trainingDate" type="date-time"></field>
        <field name="trainingPrice" type="number-integer"></field>
        <field name="trainingDuration" type="number-integer"></field>
    </entity>
</entities>